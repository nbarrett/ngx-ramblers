    "e2e": "ng e2e",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "postbuild": "npm install --prefix server",
    "serve": "ng serve --project ngx-ramblers --configuration development",
    "test": "ng test"
  },

{
  "name": "ngx-ramblers",
  "description": "client application supporting ramblers group website example: https://www.ekwg.co.uk",
  "version": "2.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "prod": "concurrently --kill-others-on-fail \"npm run start:prod\" \"npm run serve\"",
    "dev": "concurrently --kill-others-on-fail \"npm run start:test\" \"npm run serve\"",
    "dev-2": "concurrently --kill-others-on-fail \"npm run start:dev-2\" \"npm run serve\"",
    "analyze": "webpack-bundle-analyzer dist/ngx-ramblers/stats-es2015.json",
    "build:stats": "ng build --project ngx-ramblers --configuration production --statsJson=true",
    "build": "ng build --project ngx-ramblers --build-optimizer --progress --configuration production",
    "clean": "rm -rf node_modules",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "postbuild": "npm install --prefix server",
    "postinstall": "ngcc",
    "serve": "ng serve --project ngx-ramblers --configuration development",
    "start:prod": "server/scripts/run-prod.sh",
    "start:test": "server/scripts/run-test.sh",
    "start:dev-2": "server/scripts/run-dev-2.sh",
    "test": "ng test"
  },
  "cacheDirectories": [
    "node_modules",
    "server/node_modules"
  ],
  "dependencies": {
    "@angular/animations": "14.1.3",
    "@angular/cdk": "14.1.3",
    "@angular/common": "14.1.3",
    "@angular/compiler": "14.1.3",
    "@angular/core": "14.1.3",
    "@angular/forms": "14.1.3",
    "@angular/platform-browser": "14.1.3",
    "@angular/platform-browser-dynamic": "14.1.3",
    "@angular/router": "14.1.3",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@fortawesome/fontawesome-svg-core": "6.1.0",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.0",
    "@ng-select/ng-select": "8.1.1",
    "@types/lodash-es": "4.17.3",
    "angular2-csv": "0.2.9",
    "aws-sdk": "2.596.0",
    "bootstrap": "4.6.2",
    "copyfiles": "2.1.1",
    "core-js": "3.6.1",
    "express": "4.17.3",
    "express-session": "1.17.0",
    "instagram-node": "0.5.8",

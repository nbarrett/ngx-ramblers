    "moment-timezone": "0.5.27",
    "mongoose": "5.10.11",
    "ng-csv": "0.3.6",
    "ng2-file-upload": "1.4.0",
    "ngx-bootstrap": "9.0.0",
    "ngx-image-cropper": "6.2.2",
    "ngx-logger": "4.1.9",
    "ngx-markdown": "14.0.1",
    "ngx-tagify": "0.7.0",
    "ngx-ui-switch": "13.0.2",

    "mongoose-unique-validator": "5.0.1",
    "morgan": "1.9.1",
    "multer": "1.3.0",
    "npm-failsafe": "1.3.0",
    "open": "6.0.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "puppeteer": "24.4.0",
    "rand-token": "0.4.0",

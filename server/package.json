    "express-session": "1.17.3",
    "fs": "0.0.1-security",
    "fs-extra": "11.3.2",
    "he": "1.2.0",
    "inquirer": "12.8.2",
    "js-yaml": "3.13.1",
    "json2csv": "3.11.5",
    "jsonwebtoken": "8.5.1",
    "luxon": "3.7.2",
    "matchdep": "0.3.0",
    "migrate-mongo": "12.1.3",
    "mkdirpsync": "0.1.0",
    "mocha": "11.1.0",

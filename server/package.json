    "rimraf": "3.0.2",
    "serve-favicon": "2.5.0",
    "serve-index": "1.9.1",
    "sharp": "0.33.5",
    "sinon": "8.1.1",
    "source-map-support": "0.5.19",
    "ts-node": "10.9.2",

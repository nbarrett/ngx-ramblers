{
  "name": "ngx-ramblers",
  "description": "Server application to support ngx-ramblers application",
  "version": "3.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "aws-backup": "ts-node aws-backup/backup-s3-images.ts",
    "build": "npm run clean-ts && npm run tsc && npm run copy-assets",
    "clean": "rimraf target/site/serenity",
    "clean-node": "rm -rf node_modules",
    "clean-ts": "rimraf ts-gen",
    "copy-assets": "cp -r lib/geojson ts-gen/server/lib/ && cp lib/mongo/migrations/README.md ts-gen/server/lib/mongo/migrations/database/",
    "deploy": "ts-node deploy/deploy-to-environments.ts",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json --format stylish",
    "lintfix": "tslint --config ./tslint.json --project ./tsconfig.json --format stylish --fix",
    "lintversion": "tslint --version",
    "manage-configs": "ts-node deploy/manage-configs.ts",
    "mongo-cli": "tsx deploy/mongo-cli.ts",
    "mongo-cli:interactive": "tsx deploy/mongo-cli-interactive.ts",
    "mongo:backup": "tsx deploy/mongo-cli.ts backup",
    "mongo:restore": "tsx deploy/mongo-cli.ts restore",
    "migrate": "migrate-mongo up -f migrations/migrate-mongo-config.ts",
    "migrate:create": "migrate-mongo create -f migrations/migrate-mongo-config.ts",
    "migrate:down": "migrate-mongo down -f migrations/migrate-mongo-config.ts",
    "migrate:mark-old-applied": "ts-node lib/mongo/migrations/mark-migrations-applied.ts",
    "migrate:status": "migrate-mongo status -f migrations/migrate-mongo-config.ts",
    "postinstall": "tsc && npm run copy-assets",
    "postreport": "mkdir -p target/site/archive && tar -czf target/site/archive/serenity-reports.zip -C target/site/serenity .",
    "reinstall": "npm run clean-node && npm install",
    "report": "serenity-bdd run --features lib/serenity-js/features",
    "serenity": "failsafe clean serenity-run report",
    "serenity-bdd-update": "serenity-bdd update --ignoreSSL",
    "serenity-run": "wdio run ./wdio.conf.ts",
    "server": "node --optimize_for_size --max_old_space_size=460 ts-gen/server/lib/server",
    "server-live": "tsx watch lib/server",
    "test": "failsafe serenity report",
    "tsc": "tsc --project ./tsconfig.json",
    "tsc-check": "tsc --project ./tsconfig.json --noEmit",
    "tsc-watch": "tsc --project ./tsconfig.json --watch",
    "unit-test": ". test-setenv.sh && npm run build && mocha --watch",
    "unit-test:once": "bash -lc 'source ./test-setenv.sh && npm run build && mocha'"
  },
  "nodemonConfig": {
    "ignore": [
      "*.log",
      "target/**/*"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.180.0",
    "@getbrevo/brevo": "2.0.0-beta.4",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@serenity-js/console-reporter": "3.31.12",
    "@serenity-js/core": "3.31.12",
    "@serenity-js/mocha": "3.31.12",
    "@serenity-js/serenity-bdd": "3.31.12",
    "@serenity-js/web": "3.31.12",
    "@serenity-js/webdriverio": "3.31.12",
    "@turf/turf": "7.2.0",
    "@turf/union": "7.2.0",
    "@types/bcryptjs": "2.4.6",
    "@types/debug": "4.1.12",
    "@types/expect": "24.3.2",
    "@types/express-serve-static-core": "4.17.39",
    "@types/inquirer": "9.0.8",
    "@types/jsonwebtoken": "9.0.6",
    "@types/luxon": "3.7.1",
    "@types/mailchimp__mailchimp_marketing": "3.0.9",
    "@types/mocha": "10.0.10",
    "@types/mongoose": "5.11.97",
    "@types/multer": "1.4.11",
    "@types/node": "20.8.1",
    "@types/passport": "1.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/sinon": "4.3.3",
    "@types/sinon-chai": "3.2.2",
    "@wdio/cli": "9.12.0",
    "@wdio/globals": "9.12.0",
    "@wdio/local-runner": "9.12.0",
    "@wdio/reporter": "9.11.0",
    "@wdio/spec-reporter": "9.11.0",
    "angular-logger": "1.3.2",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "bottleneck": "2.19.5",
    "commander": "14.0.0",
    "compression": "1.7.4",
